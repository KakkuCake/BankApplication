<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".CreateNewCreditAccountActivity"></activity>
        <activity android:name=".ChangeCreditLimitActivity" />
        <activity android:name=".CreditAccountActivity" />
        <activity android:name=".AddMoneyActivity" />
        <activity android:name=".AccountTransfer" />
        <activity android:name=".TransferActivity" />
        <activity android:name=".RegularAccountActivity" />
        <activity android:name=".CreateNewAccountActivity" />
        <activity android:name=".CreateNewRegularAccountActivity" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".EditProfileActivity" />
    </application>

</manifest>